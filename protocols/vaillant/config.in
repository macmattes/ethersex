usart_count_used
if [ "$VAILLANT_SUPPORT" = y -o $USARTS -gt $USARTS_USED ]; then
	dep_bool_menu "VAILLANT Support" VAILLANT_SUPPORT $ARCH_AVR
		choice '  VAILLANT usart select' "$(usart_choice VAILLANT)"
		usart_process_choice VAILLANT
		comment  "Debugging Flags"
		dep_bool 'VAILLANT' DEBUG_VAILLANT $DEBUG
	endmenu
else
	define_bool VAILLANT_SUPPORT n
	comment "VAILLANT not available. No free usart. ($USARTS_USED/$USARTS)"
fi
